{
"manifest_version": 2,
    "name": "Clyde Numberer",
    "description": "Numbering your Clyde viewings",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
